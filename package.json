{
  "name": "openseadragon-fabric-overlay",
  "version": "1.1.3",
  "description": "A OpenSeaDragon plugin to allow the use of FabricJS overlay",
  "type": "module",
  "main": "dist/fabric-overlay.js",
  "types": "dist/fabric-overlay.d.ts",
  "scripts": {
    "clean": "rimraf ./dist ./site/dist",
    "build": "npm run clean && tsc",
    "dev": "vite --config site/vite.config.ts",
    "build:demo": "npm run build && vite build --config site/vite.config.ts --mode prod",
    "deploy:demo": "npm run build && npm run build:demo && git subtree push --prefix site/dist origin gh-pages",
    "test": "echo 'No tests are added'",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rssaini01/openseadragon-fabric-overlay.git"
  },
  "keywords": [
    "openseadragon",
    "osd",
    "plugin",
    "fabric",
    "fabricjs",
    "overlay"
  ],
  "author": "Ravi Shankar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rssaini01/openseadragon-fabric-overlay/issues"
  },
  "homepage": "https://github.com/rssaini01/openseadragon-fabric-overlay#readme",
  "devDependencies": {
    "@types/openseadragon": "^4.1.0",
    "fabric": "^6.7.1",
    "gh-pages": "^6.3.0",
    "openseadragon": "^5.0.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.6"
  }
}
